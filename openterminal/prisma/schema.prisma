datasource db {
  provider = "mysql"
  url = env("DB_GENOPI")
  referentialIntegrity = "prisma"
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model Test {
  id      Int   @default(autoincrement()) @id
  name    String
  creator    String
  questions    String
  date   String
}